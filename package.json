{
    "name": "stocksense",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:web": "npm run dev --workspaces=apps/web",
        "dev:api": "npm run dev --workspaces=apps/api",
        "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\""
    },
    "devDependencies": {
        "concurrently": "^9.2.1",
        "dotenv-cli": "^11.0.0"
    }
}
